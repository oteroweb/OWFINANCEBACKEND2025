  ROLES ||--o{ USERS : has
  CURRENCIES ||--o{ USERS : default_currency
  CLIENTS ||--o{ USERS : belongs_to
  USERS ||--o{ SESSIONS : has

  CURRENCIES ||--o{ ACCOUNTS : uses
